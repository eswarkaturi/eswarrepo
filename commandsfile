git add file1  (workspace to staging/index)
git reset HEAD file  (staging/index to workspace)
34 PM: 
git log
git log --oneline
git log -3
git log --oneline -4
git log --author=prasad
git log --since=yy-mm-dd
or
git log --after="2018-12-07"
git log --befor="2019-02-07"
or
git log --until=yy-mm-dd

git log --after="yesterday"

git log --since=yy-mm-dd --until=yy-mm-dd
or
git log --after=yy-mm-dd --before=yy-mm-dd

git log --grep="msg"
git log -- file-name
git log --after="2019-06-28 20:26" --before="2019-06-29 20:30"
git commit -m "label"  (staging to local)
git reset --soft cid   (local to staging)

